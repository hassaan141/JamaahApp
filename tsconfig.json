{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "jsx": "react-jsx",
    "lib": ["ES2021"],
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/types": ["src/types/index.ts"]
    },
    "types": ["react", "react-native"]
  },
  "include": ["**/*.ts", "**/*.tsx", "app.config.ts", "index.ts"],
  "extends": "expo/tsconfig.base"
}
